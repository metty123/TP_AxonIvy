Rs0 f28 actionCode 'import ch.ivyteam.ivy.admin.tool.restclient.RestClientDTOService;
import ch.ivyteam.ivy.rest.client.authentication.HttpDigestAuthenticationFeature;
import ch.ivyteam.ivy.rest.client.authentication.HttpBasicAuthenticationFeature;
import ch.ivyteam.ivy.rest.client.authentication.HttpBasicOrDigestAuthenticationFeature;

String httpBasicFeatureClassName = HttpBasicAuthenticationFeature.class.getName();
String httpDigestFeatureClassName = HttpDigestAuthenticationFeature.class.getName();
String httpBasicOrDigestFeatureClassName = HttpBasicOrDigestAuthenticationFeature.class.getName();
RestClientDTOService service = new RestClientDTOService(in.restClient);
if(panel.httpBasicType.isSelected() && panel.httpDigestType.isSelected()) {
	service.addFeature(httpBasicOrDigestFeatureClassName);
	service.removeFeatureByName(httpBasicFeatureClassName);
	service.removeFeatureByName(httpDigestFeatureClassName);
} else {
	service.removeFeatureByName(httpBasicOrDigestFeatureClassName);
}' #txt
