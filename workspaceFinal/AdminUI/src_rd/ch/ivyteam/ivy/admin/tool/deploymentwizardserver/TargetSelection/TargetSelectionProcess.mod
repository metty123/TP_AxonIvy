Ts0 f99 actionCode 'import ch.ivyteam.ivy.application.IApplication;
import ch.ivyteam.ivy.admin.tool.deploymentwizardserver.common.LibraryUtil;
import ch.ivyteam.ivy.application.ReleaseState;
import ch.ivyteam.ivy.application.ActivityState;
import ch.ivyteam.ivy.admin.tool.deploymentwizardserver.ProjectInfo;

// XIVYADMUI-6: Activate/Release ALL buttons
// Get the state of each PMVs, then select/deselect the "XY All" checkboxes accordingly
boolean activeAll = true;
boolean releaseAll = true;

for (ProjectInfo item: in.deployProjectList){
	if (!item.getActivity().isEmpty() && !item.getActivity().equals(ActivityState.ACTIVE.name())) {
		activeAll = false;
	}
	
	if (!item.getRelease().isEmpty() && !item.getRelease().equals(ReleaseState.RELEASED.name())) {
		releaseAll = false;
	}
	
	IApplication deployApp;
	
	if (item.getActivity().isEmpty()) {
		for (IApplication app : in.appList) {
			if (app.getName().equals(item.getApplication())) {
				deployApp = app;
				break;
			}
		}
	}
	
	if (deployApp is initialized) {
		if ((item.getActivity().isEmpty() && ActivityState.ACTIVE != LibraryUtil.getDefaultActivityState(deployApp, item.processModel))) {
			activeAll = false;
		}
		
		if ((item.getRelease().isEmpty() && ReleaseState.RELEASED != LibraryUtil.getDefaultReleaseState(deployApp, item.processModel))) {
			releaseAll = false;
		}
	}
}

panel.ckActivateAll.setSelected(activeAll);
panel.ckReleaseAll.setSelected(releaseAll);' #txt
