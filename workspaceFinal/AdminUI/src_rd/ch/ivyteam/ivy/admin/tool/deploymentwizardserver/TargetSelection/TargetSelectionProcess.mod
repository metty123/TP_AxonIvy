Ts0 f5 actionCode 'import ch.ivyteam.ivy.application.IProcessModelVersion;
import ch.ivyteam.ivy.application.IProcessModel;
import ch.ivyteam.ivy.security.PermissionDeniedException;
import com.ulcjava.base.application.util.Font;
import ch.ivyteam.ivy.server.ServerFactory;
import ch.ivyteam.ivy.application.IApplication;
import ch.ivyteam.ivy.richdialog.widgets.components.RFiller;
import ch.ivyteam.ivy.admin.tool.deploymentwizardserver.ProjectInfo;
import com.ulcjava.base.application.GridBagConstraints;
import ch.ivyteam.ivy.richdialog.widgets.components.RCheckBox;

in.appList.clear();
in.applyManualSelection = true;
panel.gridBagLayoutPane.removeAll();
in.errorMessage = "";

// get available apps on server
try 
{
	List<IApplication> apps = ServerFactory.getServer().getApplicationConfigurationManager().getApplicationsSortedByName(false);
	for (IApplication app: apps)
	{
		in.appList.add(app);
	}
} 
catch(PermissionDeniedException ppe)
{
	in.errorMessage = ppe.getMessage();
} 
catch(NullPointerException npe)
{
	in.errorMessage = npe.getMessage();
}

// check for case that appling manual selection from Admin UI tree is possible

if (in.appsFromAdmin.size() == 0)
{
	in.applyManualSelection = false;
}
// more than 2 project deploy to Pm, pmv ....
if (in.deployProjectList.size() > 1 && (in.pmsFromAdmin.size() > 0 || in.pmvsFromAdmin.size() > 0))
{
	in.applyManualSelection = false;
}
// 1 project deploy to more than 2 pms / pmvs in same Application
if (in.deployProjectList.size() == 1)
{	
	for (int i = 0; i < in.pmsFromAdmin.size() - 1; i++)
	{
		String appName = in.pmsFromAdmin.get(i).getApplication().getName();
		for (int j = i + 1; j < in.pmsFromAdmin.size(); j++)
		{
			if (appName.equals(in.pmsFromAdmin.get(j).getApplication().getName()))
			{
				in.applyManualSelection = false;
				break;
			}
		}
	}
	
	for (int i = 0; i < in.pmvsFromAdmin.size() - 1; i++)
	{
		String appName = in.pmvsFromAdmin.get(i).getApplication().getName();
		for (int j = i + 1; j < in.pmvsFromAdmin.size(); j++)
		{
			if (appName.equals(in.pmvsFromAdmin.get(j).getApplication().getName()))
			{
				in.applyManualSelection = false;
				break;
			}
		}
	}
}

if(in.errorMessage.isEmpty())
{	
	Font font = panel.gridBagLayoutPane.getFont().deriveFont(Font.BOLD);
	
	if (in.appList.size() > 0) 
	{
		// add first check box to select all/deselect all
		RCheckBox firstCheckBox = new RCheckBox();
		firstCheckBox.setSelected(false);
		panel.addActionListenerForFirstCheckbox(firstCheckBox);
		
		GridBagConstraints gridBagConstraint = new GridBagConstraints();
		gridBagConstraint.setAnchor(GridBagConstraints.WEST);
		gridBagConstraint.setGridX(0);
		gridBagConstraint.setGridY(0);
		firstCheckBox.setText(ivy.cms.co("/ch/ivyteam/ivy/server/deploymentWizard/component/buttonName/selectAll"));
	 	firstCheckBox.setFont(font);
		
		panel.gridBagLayoutPane.add(firstCheckBox, gridBagConstraint);
		firstCheckBox.requestFocus();
	}
	
	for (int i = 0; i < in.appList.size(); i++)
	{
		RCheckBox myCheckBox = new RCheckBox();
		myCheckBox.setText(in.appList.get(i).getName());
		// Check if app''s name existed in list of last selected apps
		if (in.applyManualSelection == false)
		{
			for (String selectedApp : in.selectedApps) 
			{
				if (selectedApp.equals(in.appList.get(i).getId().toString())) 
				{
					myCheckBox.setSelected(true);
					break;
				}
			}
		}
		else 
		{
			for (IApplication app : in.appsFromAdmin) 
			{
				if (app.getName().equals(in.appList.get(i).getName())) 
				{
					myCheckBox.setSelected(true);
					break;
				}
			}
		}
		
		panel.addActionListener(myCheckBox);
			
		GridBagConstraints gridBagConstraint = new GridBagConstraints();
		gridBagConstraint.setAnchor(GridBagConstraints.WEST);
		gridBagConstraint.setGridX(0);
		gridBagConstraint.setGridY(i + 1);
			
		panel.gridBagLayoutPane.add(myCheckBox, gridBagConstraint);
	}
		
	// Add filler to align top
	RFiller my1stFiller = new RFiller();
	GridBagConstraints my1stConstraint = new GridBagConstraints();
	my1stConstraint.setFill(GridBagConstraints.VERTICAL);
	my1stConstraint.setGridX(0);
	my1stConstraint.setWeightY(1);
	panel.gridBagLayoutPane.add(my1stFiller, my1stConstraint);
		
	// Add filler to align left
	RFiller my2ndFiller = new RFiller();
	GridBagConstraints my2ndConstraint = new GridBagConstraints();
	my2ndConstraint.setGridX(1);
	my2ndConstraint.setGridY(0);
	my2ndConstraint.setFill(GridBagConstraints.HORIZONTAL);
	my2ndConstraint.setWeightX(1);
	panel.gridBagLayoutPane.add(my2ndFiller, my2ndConstraint);
	
	//panel.setEnableNextButton();
}' #txt
