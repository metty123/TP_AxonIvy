Ts0 f21 actionCode 'import com.ulcjava.base.application.ULCComponent;
import ch.ivyteam.ivy.richdialog.widgets.components.RCheckBox;

in.hasAnyCheckBoxSelected = false;
boolean isAllSelected = true;

List<ULCComponent> components = panel.gridBagLayoutPane.getComponents();

for (int i = 1; i < components.size(); i++){
	ULCComponent component = components.get(i);
	if(component instanceof RCheckBox){
		RCheckBox checkBox = component as RCheckBox;
		if(checkBox.isSelected()){
			in.hasAnyCheckBoxSelected =	checkBox.isSelected();
		} else {
			isAllSelected = false;
		}
	}
}


ULCComponent firstComponent = components.get(0);
if(firstComponent instanceof RCheckBox){
	RCheckBox checkBox = firstComponent as RCheckBox;
	// if all is selected, select first checkbox
	if (isAllSelected){
		checkBox.setSelected(true);	
	} else {
		checkBox.setSelected(false);
	}
}' #txt
