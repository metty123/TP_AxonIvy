ds0 f122 actionCode 'import ch.ivyteam.ivy.admin.tool.deploymentwizardserver.FileBrowserUtil;
import ch.ivyteam.ivy.admin.tool.deploymentwizardserver.ProjectInfo;
import ch.ivyteam.ivy.admin.tool.deploymentwizardserver.ProjectExtractor;

// No exception
if (in.projectExtractor.getException().isEmpty())
{
	List<ProjectInfo> extractedProjects = in.projectExtractor.getProjects();
	boolean isDifference = false;
		
	if(extractedProjects.size() == in.projectList.size())
	{
		for(int i = 0; i < extractedProjects.size(); i++){
			ProjectInfo oldProject = in.projectList.get(i);
			ProjectInfo newProject = extractedProjects.get(i);
			if(!oldProject.getProjectName().equals(newProject.getProjectName()) ||
			   !oldProject.getQualifiedProjectVersion().equals(newProject.getQualifiedProjectVersion()) ||
				 !oldProject.getLibraryId().equals(newProject.getLibraryId()) ||
				 !oldProject.getProjectPath().equals(newProject.getProjectPath()) ||
				 !oldProject.getRequireProjects().equals(newProject.getRequireProjects()))
			{
				isDifference = true;
				break;
			}
		}
	} 
	else 
	{
		isDifference = true;
	}

	//refresh the selected list
	if(extractedProjects!=null && extractedProjects.size() > 0) 
	{
		in.projectList.clear();
		in.projectList.addAll(extractedProjects.deepClone());
	}

	// Only reload the project list in case has any changes
	if (isDifference) 
	{
		panel.loadProjects(extractedProjects);
	}
}
else 
{
	// show error dialog
	panel.showErrorDialog(in.projectExtractor.getException());
}

// Check for invalid path on server
if (in.selectedPathOnServer.isEmpty() == false && panel.radioServer.isSelected())
{
	boolean isFileExist = FileBrowserUtil.isFileExisting(in.selectedPathOnServer);
	{
		if (isFileExist == false)
		{
			panel.showInvalidPathError();
		}
	}
}

//Call restart Progress after abortion called
in.projectExtractor.restartProgress();

panel.updateUIAfterProgress();' #txt
