ds0 f18 actionCode 'import java.util.ArrayList;
import ch.ivyteam.ivy.admin.tool.deploymentwizardserver.common.MessageFactory;
import ch.ivyteam.ivy.admin.tool.deploymentwizardserver.common.DuplicatedProjectDetector;
import ch.ivyteam.ivy.admin.tool.deploymentwizardserver.ProjectInfo;
import com.ulcjava.base.application.ULCComponent;
import ch.ivyteam.ivy.richdialog.widgets.components.RCheckBox;

List<ULCComponent> components = panel.gridBagLayoutPane.getComponents();
RCheckBox selectAllCheckBox = components.get(0) as RCheckBox;
components.remove(selectAllCheckBox);

int projectIndex = 0;
in.selectedProjects.clear();
List<ProjectInfo> duplicatedProjects = new List<ProjectInfo>();

for (ULCComponent component : components) {
	if(component instanceof RCheckBox){
		RCheckBox checkBox = component as RCheckBox;
		ProjectInfo projectInfo = in.projectList.get(projectIndex);
		List<ProjectInfo> projects = new ArrayList<ProjectInfo>(in.projectList);
		projects.remove(projectInfo);
		if (DuplicatedProjectDetector.hasDuplicatedProject(projects, projectInfo)) {
			checkBox.setSelected(false);			
			duplicatedProjects.add(projectInfo);
		} else {
			in.selectedProjects.add(projectInfo);
			checkBox.setSelected(true);
		}
		projectIndex++;
	}
}

in.hasSameProject = !duplicatedProjects.isEmpty();

// unselect "Select All" check box when has duplicated project
selectAllCheckBox.selected = duplicatedProjects.isEmpty();

in.errorMessage = MessageFactory.createDuplicatedProjectErrorMessage(duplicatedProjects);' #txt
