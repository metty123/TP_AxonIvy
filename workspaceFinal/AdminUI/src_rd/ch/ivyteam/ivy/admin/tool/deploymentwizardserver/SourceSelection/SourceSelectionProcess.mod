ds0 f79 actionCode 'import com.ulcjava.base.application.ClientContext;

in.attributes.clear();
in.attributes.add("selectedPathOnLocal");
in.attributes.add("selectedPathOnServer");
in.attributes.add("isServerMode");

if (ivy.session.getSessionUser() != null && in.attributes.size() >= 3) 
{
	// Restore last settings
	in.selectedPathOnLocal = ivy.rd.getContext().getAttribute(in.attributes.get(0));
	in.selectedPathOnServer = ivy.rd.getContext().getAttribute(in.attributes.get(1));

	String storedValue = ivy.rd.getContext().getAttribute(in.attributes.get(2));
	// Change selection of selection source mode if stored setting is different from default setting

	if (storedValue != null && storedValue.equalsIgnoreCase("true")) 
	{
		panel.radioServer.selected = true;
		in.isKeptServerUserSelection = true;
		in.isFromServer = panel.radioServer.isSelected();
		panel.txtSelectRootDirectory.setText(in.selectedPathOnServer);
	} else {
		panel.txtSelectRootDirectory.setText(in.selectedPathOnLocal);
	}
}' #txt
