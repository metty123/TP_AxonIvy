ds0 f14 actionCode 'import ch.ivyteam.ivy.admin.tool.deploymentwizardserver.ProjectExtractor;

String path = panel.txtSelectRootDirectory.getText().trim();
// Browser on local PC
if (panel.radioLocal.isSelected())
{
 // update to store settings
	in.selectedPathOnLocal = path;
		
	panel.browseClientProjects(path);	
} 
// Browser on server PC
else if(panel.radioServer.isSelected()) 
{ 
	// update to store settings
	in.selectedPathOnServer = path; 
	// get list of projects from a separated thread
	in.projectExtractor = new ProjectExtractor(path);
	in.projectExtractor.start();
	panel.startPollingTimer();
}

in.isEnterOrButtonClicked = false;
in.isEnterClicked = false;

