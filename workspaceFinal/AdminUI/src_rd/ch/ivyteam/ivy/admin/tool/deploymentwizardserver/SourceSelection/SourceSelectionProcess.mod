ds0 f44 outActionCode 'import java.util.UUID;
import ch.ivyteam.ivy.admin.tool.deploymentwizardserver.ProjectInfo;
import java.util.ArrayList;
import com.ulcjava.base.application.ULCComponent;
import ch.ivyteam.ivy.richdialog.widgets.components.RCheckBox;

List<ULCComponent> components = panel.gridBagLayoutPane.getComponents();
List<RCheckBox> checkBoxList = new ArrayList();

// clear and add all project to return project list
result.projectList.clear();
result.projectList.addAll(in.projectList.deepClone());

// First checkbox is used to select/deselect all projects
for (int i = 1; i < components.size(); i++) 
{
	ULCComponent component = components.get(i);
	
	if(component instanceof RCheckBox)
	{
		RCheckBox checkBox = component as RCheckBox;
		checkBoxList.add(checkBox);
	}
}

// with check box which is uncheck, remove equivalent project info from list
if (result.projectList.size() == checkBoxList.size())
{
	for (int i = checkBoxList.size() - 1; i >= 0; i --)
	{
		// remove project info object which check box is not checked
		RCheckBox checkBox = checkBoxList.get(i) as RCheckBox;
		if(checkBox.isSelected() == false)
		{
			result.projectList.removeAt(i);
		} 
	}
}

for (ProjectInfo item: result.projectList)
{
	item.setId(UUID.randomUUID().toString());
}








