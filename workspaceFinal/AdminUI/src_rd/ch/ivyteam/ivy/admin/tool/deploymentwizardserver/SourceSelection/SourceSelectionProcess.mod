ds0 f30 actionCode 'import com.ulcjava.base.application.util.Font;
import ch.ivyteam.ivy.richdialog.widgets.components.RFiller;
import com.ulcjava.base.application.GridBagConstraints;
import ch.ivyteam.ivy.richdialog.widgets.components.RCheckBox;
import ch.ivyteam.ivy.admin.tool.deploymentwizardserver.ProjectInfo;


// remove the list & disable the "Next" button
panel.gridBagLayoutPane.removeAll();

int listProjectInfoSize = in.projectList.size();

if (listProjectInfoSize > 0) {
	// add first check box to select all/deselect all
	RCheckBox firstCheckBox = new RCheckBox();
	firstCheckBox.setText(ivy.cms.co("/ch/ivyteam/ivy/server/deploymentWizard/component/buttonName/selectAll"));
	firstCheckBox.setFont(panel.gridBagLayoutPane.getFont().deriveFont(Font.BOLD));
	firstCheckBox.setSelected(false);
	
	GridBagConstraints gridBagConstraint = new GridBagConstraints();
	gridBagConstraint.setAnchor(GridBagConstraints.WEST);
	gridBagConstraint.setGridX(0);
	gridBagConstraint.setGridY(0);
	
	panel.gridBagLayoutPane.add(firstCheckBox, gridBagConstraint);
	firstCheckBox.requestFocus();
	
	ivy.rd.addEventMapping(firstCheckBox, "all_projects_checkbox_selected");
}

in.projectMappedByCheckBoxIds = new java.util.HashMap();

// add check box for every found project
for (int i = 0; i < listProjectInfoSize; i++){	
	ProjectInfo projectInfo = in.projectList.get(i);

	RCheckBox myCheckBox = new RCheckBox();
	myCheckBox.setText(projectInfo.getProjectName() + " (" + projectInfo.getProjectPath() + ")");
	myCheckBox.setSelected(false);
	
	GridBagConstraints gridBagConstraint = new GridBagConstraints();
	gridBagConstraint.setAnchor(GridBagConstraints.WEST);
	gridBagConstraint.setGridX(0);
	gridBagConstraint.setGridY(i + 1);
	
	panel.gridBagLayoutPane.add(myCheckBox, gridBagConstraint);
	
	in.projectMappedByCheckBoxIds.put(myCheckBox.getId(), projectInfo);
	ivy.rd.addEventMapping(myCheckBox, "checkbox_selected");
}

// Add filler to align top
RFiller my1stFiller = new RFiller();
GridBagConstraints my1stConstraint = new GridBagConstraints();
my1stConstraint.setFill(GridBagConstraints.VERTICAL);
my1stConstraint.setGridX(0);
my1stConstraint.setWeightY(1);
panel.gridBagLayoutPane.add(my1stFiller, my1stConstraint);

// Add filler to align left
RFiller my2ndFiller = new RFiller();
GridBagConstraints my2ndConstraint = new GridBagConstraints();
my2ndConstraint.setGridX(1);
my2ndConstraint.setGridY(0);
my2ndConstraint.setFill(GridBagConstraints.HORIZONTAL);
my2ndConstraint.setWeightX(1);
panel.gridBagLayoutPane.add(my2ndFiller, my2ndConstraint);

// set enable Next button
panel.setEnableNextButton();

in.isFromServer = panel.radioServer.isSelected();
in.selectedProjects.clear();' #txt
